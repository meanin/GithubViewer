{
  "swagger": "2.0",
  "info": {
    "description": "Api for parsing github data",
    "version": "0.0.1",
    "title": "Swagger Github-Viewer",
    "contact": {
      "email": "meanin@wp.pl"
    }
  },
  "host": "github-viewer.swagger.io",
  "basePath": "/swagger",
  "tags": [
    {
      "name": "Github-Viewer",
      "description": "Retreive data from github api and pass it forward serialized as you want",
      "externalDocs": {
        "description": "Github-Viewer Project",
        "url": "https://github.com/meanin/GithubViewer"
      }
    }
  ],
  "schemes": [
    "http",
    "https"
  ],
  "paths": {
    "/api/github-viewer/user/{login}": {
      "get": {
        "tags": [
          "Github-Viewer"
        ],
        "summary": "Gets user info of given login",
        "description": "Returns serialized object, based on given acceptance header, into GithubUser",
        "operationId": "GetUser",
        "produces": [
          "application/xml",
          "application/json",
          "application/simplecsv"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "login",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "$ref": "#/definitions/githubuser"
            }
          },
          "401": {
            "description": "NotFound"
          }
        }
      }
    },
    "/api/github-viewer/user/{login}/{repository}": {
      "get": {
        "tags": [
          "Github-Viewer"
        ],
        "summary": "Gets user info of given login",
        "description": "Returns serialized object, based on given acceptance header, into GithubUser",
        "operationId": "GetRepository",
        "produces": [
          "application/xml",
          "application/json",
          "application/simplecsv"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "login",
            "required": true,
            "type": "string"
          },
          {
            "in": "path",
            "name": "repository",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "$ref": "#/definitions/githubrepositorydetails"
            }
          },
          "401": {
            "description": "NotFound"
          }
        }
      }
    }
  },
  "definitions": {
    "githubuser": {
      "type": "object",
      "properties": {
        "Id": {
          "type": "integer",
          "format": "int32",
          "example": 6974668
        },
        "Login": {
          "type": "string",
          "example": "meanin"
        },
        "AvatarUrl": {
          "type": "string",
          "example": "http://someurl.com"
        },
        "BlogUrl": {
          "type": "string",
          "example": "http://someurl.com"
        },
        "Location": {
          "type": "string",
          "example": "internet"
        },
        "StarredRepositoryList": {
          "type": "object",
          "$ref": "#/definitions/repositorylist"
        },
        "RepositoryList": {
          "type": "object",
          "$ref": "#/definitions/repositorylist"
        }
      },
      "xml": {
        "name": "GithubUser"
      }
    },
    "githubrepository": {
      "title": "GithubRepository",
      "type": "object",
      "properties": {
        "Id": {
          "type": "integer",
          "format": "int32",
          "example": 102251318
        },
        "Name": {
          "type": "string",
          "example": "GithubViewer"
        },
        "Language": {
          "type": "string",
          "example": "C#"
        },
        "StargazesCount": {
          "type": "integer",
          "format": "int32",
          "example": 0
        }
      },
      "xml": {
        "name": "GithubRepository"
      }
    },
    "githubrepositorydetails": {
      "allOf": [
        {
          "$ref": "#/definitions/githubrepository"
        },
        {
          "type": "object",
          "title": "GithubRepositoryDetails",
          "properties": {
            "Url": {
              "type": "string",
              "example": "https://github.com/meanin/GithubViewer"
            },
            "Created": {
              "type": "string",
              "format": "date-time",
              "example": "2017-09-03T08:35:22Z"
            },
            "LastUpdate": {
              "type": "string",
              "format": "date-time"
            }
          },
          "xml": {
            "name": "GithubRepositoryDetails"
          }
        }
      ]
    },
    "repositorylist": {
      "title": "RepositoryList",
      "type": "object",
      "properties": {
        "RepositoryList": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/githubrepository"
          }
        }
      }
    }
  },
  "externalDocs": {
    "description": "Find out more about Swagger",
    "url": "http://swagger.io"
  }
}